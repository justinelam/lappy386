alias pipdate3="pip3 list --outdated --format=freeze | grep -v '^\-e' | cut -d = -f 1  | xargs -n1 pip3 install -U"

alias jpip="`find /usr/local/Cellar/jupyterlab | grep /bin/python3$` -m pip"
alias jpipdate3="`find /usr/local/Cellar/jupyterlab | grep /bin/python3$` -m pip list --outdated --format=freeze | grep -v '^\-e' | cut -d = -f 1 | xargs -n1 `find /usr/local/Cellar/jupyterlab | grep /bin/python3$` -m pip install -U"
alias jenv="`find /usr/local/Cellar/jupyterlab | grep /bin/python3$` -m venv"

